<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <!--乱码过滤器  -->
  <filter>
  	<filter-name>LuanMaFilter</filter-name>
  	<filter-class>com.hotel.guolu.LuanMaFilter</filter-class>
  	
  </filter>
  <filter-mapping>
  	<filter-name>LuanMaFilter</filter-name>
  	<url-pattern>/servlet/*</url-pattern>
  </filter-mapping>
  
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CaidanServlet</servlet-name>
    <servlet-class>com.hotel.servlet.CaidanServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DinnerTableServlet</servlet-name>
    <servlet-class>com.hotel.servlet.DinnerTableServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FoodServlet</servlet-name>
    <servlet-class>com.hotel.servlet.FoodServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>OrderServlet</servlet-name>
    <servlet-class>com.hotel.servlet.OrderServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QFoodServlet</servlet-name>
    <servlet-class>com.hotel.qiantai.servlet.QFoodServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QOrderServlet</servlet-name>
    <servlet-class>com.hotel.qiantai.servlet.QOrderServlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>CaidanServlet</servlet-name>
    <url-pattern>/servlet/CaidanServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DinnerTableServlet</servlet-name>
    <url-pattern>/servlet/DinnerTableServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FoodServlet</servlet-name>
    <url-pattern>/servlet/FoodServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderServlet</servlet-name>
    <url-pattern>/servlet/OrderServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QFoodServlet</servlet-name>
    <url-pattern>/servlet/QFoodServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QOrderServlet</servlet-name>
    <url-pattern>/servlet/QOrderServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
